<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib"
   >

    <sys:String x:Key="SegoeMDL2Font">/Vine;component/Fonts/Fonts.zip#Segoe MDL2 Assets</sys:String>

    <SolidColorBrush Color="#E0E0E0" x:Key="BackgroundImageBrush" />
    <Color x:Key="DarkGreyColor">#FF808080</Color>
    <SolidColorBrush x:Key="DarkGreyBrush" Color="{StaticResource DarkGreyColor}" />
    <Color x:Key="GreyColor">#FFD2D2D2</Color>

    <Color x:Key="GreenColor">#4cd97e</Color>
    <SolidColorBrush x:Key="GreenBrush" Color="{StaticResource GreenColor}"/>


    <Color x:Key="RedColor">#e5705d</Color>
    <SolidColorBrush x:Key="RedBrush" Color="{StaticResource RedColor}"/>


    <SolidColorBrush x:Key="PivotThemeIconBrush" Color="{StaticResource GreyColor}" />

    <SolidColorBrush x:Key="GreyBrush" Color="{StaticResource GreyColor}" />
    <SolidColorBrush x:Key="LikeBrush" Color="#ff6c6c"/>
    <SolidColorBrush x:Key="LikeOpacityBrush" Color="#ff6c6c" Opacity="0.1"/>
    <SolidColorBrush x:Key="ShadowBrush" Color="#FFD9D9D9" />
    <SolidColorBrush x:Key="DarkerBrush" Color="#FF6B6B6B" />
    <SolidColorBrush x:Key="ThemeTextBrush" Color="#000000"/>
    <SolidColorBrush x:Key="PostBackgroundBrush" Color="#ffffff"/>
    <SolidColorBrush x:Key="PostForegroundBrush" Color="#000000"/>
    <SolidColorBrush x:Key="ModernHeaderBrush" Color="#ffffff"/>
    <SolidColorBrush x:Key="ModernForegroundBrush" Color="Black"/>


    <SolidColorBrush x:Key="BackCommandBrush" Color="#FF6B6B6B" />
    <SolidColorBrush x:Key="HeaderBottomSeparatorBrush" Color="#E0E0E0" />


    <Color x:Key="LightGreyColor">#e2e2e2</Color>
    <SolidColorBrush x:Key="LightGreyBrush" Color="{StaticResource LightGreyColor}"/>

</ResourceDictionary>